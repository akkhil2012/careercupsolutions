package com.amazon.arrays;

public class SumBetwenIndices {

	
	  public int findSumIndices(int[] arr,int i,int j){
		  int sum = 0;
		  int index = 0;
		  /*while(index < i){
			  index++;
		  }*/
		  for(index = i;index<j;index++){
			sum +=arr[index];  
		  }
		  
		  
	  }
	
	
	
		public static void main(String args[]) {
			int arr[] = { 20, 45, 67, 75, 76, 78, 83, 88, 89, 91, 98, 99 };
			System.out.println(" Sum between indices " + new SumBetwenIndices().findSumIndices(arr,2,6));
		}
	}

	

